FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      C_CO2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    surface
    {
        type       externalSpecieDiffusionResistance;
        H_cc       {H_cc};
        D_CO2_l    {D_CO2_l};
        C_CO2_g    {C_CO2_g};
        K_ext      {K_ext};
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    "wedge.*"
    {
        type            wedge;
    }
}


